<article class="speakerslist">
    <section class="layout">
        <div class="speakers">
            <% artists.forEach(function(item) { %>
            <article class="artist group">
                <h3><%= item.title %> <span class="small">with
            <a href="/developers/<%= item.shortname %>">
                <%= item.name %></a><span></h3>
                <p><a href="/developers/<%= item.shortname %>">
                        <img class="speaker" src="/images/developers/<%= item.shortname %>_tn.jpg" alt="Photo of <%= item.name %>"></a>
                    <% if (page!= 'home') { %>
                    <% if (page === 'artistList') { %>
                    <%- item.summary %>
                    <% } else { %>
                    <%- item.description %>
                    <% } %>
                    <% } %>
                    </a></p>
            </article>
            <% }); %>
        </div>
    </section>
</article>